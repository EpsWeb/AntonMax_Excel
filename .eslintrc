{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "linebreak-style": "off",
        "no-unused-vars": "warn",
        "object-curly-spacing": "off",
        "quotes": "off"
    }
}